programa
{
	

	//Exercicio 
	//Criar duas variaveis texto1 e texto2
	//Concatenar as duas variaveis para imprimir na tela os dois textos em uma unica linha
	
	funcao inicio()
	{
		escreva("Soma:", 2+4, "\n")
		escreva("Subtração:", 2-4, "\n") 
		escreva("Multiplicação:", 2*4, "\n")
		escreva("Divisão:", 2/4, "/n")
		
	}
}

